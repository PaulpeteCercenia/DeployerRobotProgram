{
  "name": "omega-prime-deployer",
  "version": "2.0.0",
  "description": "Multi-Cloud Mainnet Token Deployer",
  "main": "multi-cloud-deploy.js",
  "scripts": {
    "start": "node multi-cloud-deploy.js",
    "mainnet:all": "node multi-cloud-deploy.js",
    "mainnet:copilot": "npx ts-node grok-copilot.ts",
    "mainnet:bot-orchestrate": "node execute-bot-minting.js",
    "mainnet:verify-bots": "node verify-bot-balances.js",
    "mainnet:reannounce-controller": "node reannounce-controller.js",
    "aws:deploy": "cd terraform && terraform apply -auto-approve",
    "azure:deploy": "cd terraform && terraform apply -target=azurerm_linux_function_app.omega_deployer -auto-approve",
    "multi-cloud:deploy": "npm run aws:deploy && npm run azure:deploy && npm run mainnet:all",
    "quick-monitor": "node quick-monitor.js",
    "omega-status": "node omega-status.js",
    "moralis:setup": "node moralis-integration.js",
    "moralis:omega": "node moralis-omega-integration.js"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^3.2.1",
    "@solana/spl-token": "^0.3.9",
    "@solana/web3.js": "^1.87.6",
    "axios": "^1.12.2",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "node-fetch": "^3.3.2",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "solana",
    "token",
    "deployment",
    "mainnet",
    "aws",
    "azure",
    "multi-cloud"
  ],
  "author": "AutomataLabs",
  "license": "MIT"
}
