{
  "name": "omega-prime-token",
  "version": "1.2.0",
  "type": "commonjs",
  "scripts": {
    "dev:check": "ts-node src/utils/checkEnv.ts",
    "mainnet:create-mint": "ts-node src/createMint.ts",
    "mainnet:mint-initial": "ts-node src/mintInitial.ts",
    "mainnet:set-metadata": "ts-node src/setMetadata.ts",
    "mainnet:lock": "ts-node src/lockAuthorities.ts",
    "mainnet:withdraw-earnings": "ts-node src/withdraw_earnings.ts",
    "mainnet:rollback": "ts-node src/rollback.ts",
    "mainnet:copilot": "ts-node grok-copilot.ts",
    "mainnet:all": "npm run dev:check && npm run mainnet:create-mint && npm run mainnet:mint-initial && npm run mainnet:set-metadata && npm run mainnet:lock",
    "verify:addresses": "node comprehensive-address-verifier.js",
    "verify:demo": "node address-verifier-demo.js",
    "build": "tsc",
    "build:rust": "cargo build --manifest-path pentacle/Cargo.toml",
    "build:all": "npm run build && npm run build:rust"
  },
  "dependencies": {
    "@metaplex-foundation/mpl-token-metadata": "^3.4.0",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "dotenv": "^16.6.1",
    "ethers": "^6.15.0",
    "node-fetch": "^2.7.0"
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
